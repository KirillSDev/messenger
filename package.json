{
  "name": "chat.project",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clear": "rm -rf dist .parcel-cache",
    "start": "npm run build && nodemon -L ./server.js --open",
    "build": "parcel build ./src/index.hbs src/pages/**/*.hbs --no-cache --no-source-maps",
    "dev": "parcel src/index.hbs src/pages/**/*.hbs --no-cache --no-source-maps",
    "prepare": "husky install"
  },
  "author": "KirillSDev",
  "license": "ISC",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-typescript": "0.55.1",
    "eslint-plugin-react": "7.31.11",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "parcel": "2.8.2",
    "parcel-transformer-hbs": "1.0.4",
    "postcss-import": "15.1.0",
    "postcss-nested": "6.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "express": "4.18.2",
    "handlebars": "4.7.7"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": ["eslint --fix", "prettier --write --ignore-unknown"]
  }
}
